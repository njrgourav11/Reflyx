{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.11",
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "routes": [
    { "src": "/api/v1/(.*)", "dest": "/api/index.py" },
    { "src": "/", "dest": "/api/index.py" }
  ],
  "env": {
    "LOG_LEVEL": "INFO"
  }
}

